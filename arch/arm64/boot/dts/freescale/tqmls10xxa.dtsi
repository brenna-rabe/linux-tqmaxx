&i2c0 {
	status = "okay";

	temp-sensor@18 {
		compatible = "nxp,se97", "jedec,jc-42.4-temp";
		reg = <0x18>;
	};

	eeprom@50 {
		compatible = "atmel,24c02";
		reg = <0x50>;
		read-only;
	};

	rtc@51 {
		compatible = "nxp,pcf85063";
		reg = <0x51>;
	};

	eeprom@57 {
		compatible = "atmel,24c256";
		reg = <0x57>;
	};
};

&esdhc {
	/* eSDHC or eMMC: set by bootloader */
	non-removable;
	disable-wp;
	mmc-hs200-1_8v;
	sd-uhs-sdr104;
	sd-uhs-sdr50;
	sd-uhs-sdr25;
	sd-uhs-sdr12;
};

