&pruss_soc_bus1 {
        status = "okay";
        pruss1: pruss@4b200000 {
                status = "okay";
                pru1_0: pru@4b234000 {
                        interrupt-parent = <&pruss1_intc>;
                        interrupts = <16>, <17>;
                        interrupt-names = "vring", "kick";
                        status = "okay";
                };
                pru1_1: pru@4b238000 {
                        interrupt-parent = <&pruss1_intc>;
                        interrupts = <18>, <19>;
                        interrupt-names = "vring", "kick";
                        status = "okay";
                };
        };
};

