#include <dt-bindings/input/input.h>
#include "imx6ul-tqma6ulxl.dtsi"

/ {
	model = "TQMa6UL SOM";
};

&reg_tqma6ul_3v3 {
	vin-supply = <&reg_sw2>;
};

&reg_sw2 {
	regulator-always-on;
};

/* eMMC */
&usdhc2 {
	vmmc-supply = <&reg_sw2>;
	vqmmc-supply = <&reg_vldo4>;

	/delete-property/ no-1-8-v;
};
