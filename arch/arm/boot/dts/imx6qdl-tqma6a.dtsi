&i2c1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1_tqma6>;
	clock-frequency = <100000>;
	status = "okay";

	pf0100: pmic@8 {
		compatible = "fsl,pfuze100";
		reg = <0x8>;
	};

	lm75_48: temperature-sensor@48 {
		compatible = "national,lm75";
		reg = <0x48>;
	};

	m24c64_50: eeprom@50 {
		compatible = "st,24c64", "atmel,24c64";
		reg = <0x50>;
		pagesize = <32>;
	};
};
